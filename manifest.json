{
  "manifest_version": 2,
  "name": "Youtube Full Page View",
  "version": "1.0.0",
  "description": "Adds a button which toggles the full page view of youtube videos",
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_title": "Toggle full page view"
  },
  "permissions": ["tabs"]
}
